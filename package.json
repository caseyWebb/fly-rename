{
  "name": "taskr-rename",
  "version": "1.0.0",
  "description": "Rename plugin for Taskr",
  "license": "WTFPL",
  "repository": "https://github.com/caseyWebb/taskr-rename",
  "files": [
    "index.js"
  ],
  "keywords": [
    "taskr",
    "taskr-plugin",
    "rename"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore ./",
    "test": "jest",
    "posttest": "rm -rf test/.tmp",
    "release": "standard-version --sign && git push --follow-tags"
  },
  "author": {
    "name": "Casey Webb",
    "email": "notcaseywebb@gmail.com",
    "url": "http://github.com/caseyWebb"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-profiscience": "^5.0.2",
    "jest": "^26.0.1",
    "prettier": "^1.18.2",
    "standard-version": "^8.0.0",
    "taskr": "^1.1.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "taskr": "^1.1.0"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-profiscience"
    ],
    "env": {
      "jest": true
    },
    "parserOptions": {
      "sourceType": "script",
      "project": "./tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "jest": {
    "collectCoverage": true,
    "testMatch": [
      "<rootDir>/test/**/*.js",
      "!**/.tmp/**",
      "!**/fixtures/**"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": false,
    "singleQuote": true
  }
}
